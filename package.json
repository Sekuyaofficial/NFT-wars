{
  "name": "sekuya",
  "description": "Sekuya NFT Wars. Choose your favorite hero and win the rewards!",
  "version": "0.0.1",
  "scripts": {
    "start:dfx": "dfx start --background --clean --host \"127.0.0.1:4943\"",
    "start": "vite src/bare_frontend --config vite.config.js",
    "build": "vite build src/bare_frontend --config vite.config.js",
    "deploy:local": "dfx deploy --network=local",
    "deploy:ic": "dfx deploy --network=ic",
    "generate": "dfx generate bare_backend",
    "deploy:nft": "dfx deploy --argument \"(principal\\\"$(dfx canister id nftminter)\\\", record { logo = record { logo_type = \\\"image/png\\\"; data = \\\"\\\"; }; name = \\\"Sekuya NFT Wars\\\"; symbol = \\\"SEQ\\\"; maxLimit = 100; })\" nft",
    "deploy:nft:mainnet": "dfx deploy --network=ic --identity=rnbtest --argument \"(principal\\\"$(dfx canister --network=ic --identity=rnbtest id nftminter)\\\", record { logo = record { logo_type = \\\"image/png\\\"; data = \\\"\\\"; }; name = \\\"Sekuya NFT Wars\\\"; symbol = \\\"SNW\\\"; maxLimit = 100; })\" nft"
  },
  "dependencies": {
    "@dfinity/agent": "^0.19.3",
    "@dfinity/auth-client": "^0.19.3",
    "@dfinity/candid": "^0.19.3",
    "@dfinity/identity": "0.19.3",
    "@dfinity/ledger-icp": "^1.0.0",
    "@dfinity/nns": "^2.0.0",
    "@dfinity/nns-proto": "^1.0.0",
    "@dfinity/principal": "^0.19.3",
    "@dfinity/utils": "^1.0.0",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@fontsource/hammersmith-one": "^5.0.8",
    "@svgr/rollup": "8.1.0",
    "@types/lodash": "^4.14.201",
    "@vitejs/plugin-react": "^4.1.1",
    "azle": "^0.18.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.1.3",
    "uuid": "^9.0.0",
    "vite-plugin-svgr": "4.1.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.11.0",
    "@emotion/css": "11.11.2",
    "@types/node": "20.9.0",
